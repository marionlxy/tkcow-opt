	</delete>
  
	<select id="findOneModuleDes" parameterType="java.util.Map" resultType="java.util.Map">
		<![CDATA[ SELECT A.* ,B.WEIGHT,B.CONTENT_ID FROM ZJOW_MODULE_DES A ,ZJOW_MODULE_CONTENT B ]]>
		<include refid="moduleDesWhereCondition" />
		and A.MOD_ID=B.MOD_ID 
  	</select>
  	
  	<select id="findNewOne" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT A.*,B.MOD_TYPE,B.MOD_BYNAME,B.MOD_SEO,B.SEO_DES,B.TITLE,B.MOD_DES,B.MOD_BANNER,B.MOD_IMG FROM ZJOW_MODULE_CONTENT A,ZJOW_MODULE_DES B 
		WHERE A.MOD_ID = B.MOD_ID AND A.DELFLAG = '0'
		<if test=" content_id != null and content_id != '' and content_id != 'null' ">
			<![CDATA[ AND A.CONTENT_ID = #{content_id} ]]>
		</if>
		<if test=" mod_id != null and mod_id != '' and mod_id != 'null' ">
			<![CDATA[ AND A.MOD_ID = #{mod_id} ]]>
		</if>
	</select>
