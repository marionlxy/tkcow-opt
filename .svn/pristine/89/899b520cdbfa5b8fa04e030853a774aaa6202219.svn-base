  	<select id="getWithoutImgListModuleList" parameterType="java.util.Map" resultType="com.taikang.udp.framework.common.datastructre.impl.BaseDto">
		<![CDATA[ SELECT A.* ,B.WEIGHT,B.CONTENT_ID FROM ZJOW_MODULE_DES A ,ZJOW_MODULE_CONTENT B, ZJOW_MODULE C ]]>
			where A.MOD_ID=B.MOD_ID  AND C.MOD_ID = A.MOD_ID AND A.DELFLAG = '0' AND B.DELFLAG = '0' AND C.DELFLAG = '0'
		<if test=" modId != null and modId != '' and modId != 'null' ">
			<![CDATA[ AND C.MOD_PARENT_ID = #{modId}]]>
	  	</if>
		<if test=" title != null and title != '' and title != 'null' ">
			<![CDATA[ AND A.TITLE like concat('%', #{title}, '%')]]>
	  	</if>
	  	<if test=" startTime != null and startTime != '' and startTime != 'null' ">
            <![CDATA[ AND A.CREATED_TIME >= DATE_FORMAT(#{startTime}, '%Y-%m-%d %H:%i:%s') ]]>
        </if>
		<if test=" endTime != null and endTime != '' and endTime != 'null' ">
            <![CDATA[ AND A.CREATED_TIME <= DATE_FORMAT(#{endTime}, '%Y-%m-%d %H:%i:%s') ]]>
        </if>
        ORDER BY  B.WEIGHT DESC
  	</select>
  	<select id="getImgListModuleList" parameterType="java.util.Map" resultType="com.taikang.udp.framework.common.datastructre.impl.BaseDto">
		<![CDATA[ SELECT A.* ,B.MOD_SQUENCE FROM ZJOW_MODULE_DES A ,ZJOW_MODULE B ]]>
			where  
		<if test=" modId != null and modId != '' and modId != 'null' ">
			<![CDATA[ B.MOD_PARENT_ID = #{modId}]]>
	  	</if>
	  	AND A.MOD_ID=B.MOD_ID
	  	AND A.DELFLAG = '0' AND B.DELFLAG = '0'
        ORDER BY  B.MOD_SQUENCE DESC
  	</select>
</mapper>
